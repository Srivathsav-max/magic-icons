{
  "name": "magic-icons",
  "version": "0.0.2",
  "description": "A comprehensive React icon library with 500 customizable icons across 5 variants - perfect for modern web applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build:showcase": "tsc -b && vite build",
    "build:icons": "node scripts/generate-icons.js",
    "build:lib": "npm run build:icons && tsup",
    "build": "npm run build:lib",
    "prepublishOnly": "npm run build:lib",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "components",
    "ui",
    "magic-icons",
    "icon-library",
    "typescript",
    "nextjs",
    "react-icons"
  ],
  "author": "Jaya Raj Srivathsav Adari",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/srivathsav-max/magic-icons.git"
  },
  "bugs": {
    "url": "https://github.com/srivathsav-max/magic-icons/issues"
  },
  "homepage": "https://github.com/srivathsav-max/magic-icons#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
