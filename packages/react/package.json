{
	"name": "magic-icons",
	"version": "0.0.2",
	"description": "A comprehensive React icon library - perfect for modern web applications",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		},
		"./metadata": {
			"types": "./dist/metadata.d.ts",
			"import": "./dist/metadata.js",
			"default": "./dist/metadata.js"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": false,
	"files": [
		"dist",
		"README.md",
		"LICENSE",
		"src/components/icons/metadata.json"
	],
	"scripts": {
		"build:icons": "bun run scripts/generate-icons.mts",
		"build": "bun run build:icons && tsup",
		"prepublishOnly": "bun run build",
		"lint": "eslint .",
		"svg:check": "bun run scripts/svg-name-updater.mts",
		"svg:rename": "bun run scripts/svg-name-updater.mts rename --apply",
		"svg:sort": "bun run scripts/svg-name-updater.mts sort --apply",
		"svg:organize": "bun run scripts/svg-name-updater.mts --apply",
		"schemas:generate": "bun run scripts/generate-icon-schemas.mts",
		"migrate:check": "bun run scripts/migrate-to-categories.mts",
		"migrate:apply": "bun run scripts/migrate-to-categories.mts --apply"
	},
	"keywords": [
		"react",
		"icons",
		"svg",
		"components",
		"ui",
		"magic-icons",
		"icon-library",
		"typescript",
		"nextjs",
		"react-icons"
	],
	"author": "Jaya Raj Srivathsav Adari",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/srivathsav-max/magic-icons.git",
		"directory": "packages/react"
	},
	"bugs": {
		"url": "https://github.com/srivathsav-max/magic-icons/issues"
	},
	"homepage": "https://github.com/srivathsav-max/magic-icons#readme",
	"peerDependencies": {
		"react": ">=17.0.0",
		"react-dom": ">=17.0.0"
	},
	"devDependencies": {
		"@types/node": "^24.9.2",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.7",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tsup": "^8.5.0",
		"typescript": "~5.9.3"
	}
}
