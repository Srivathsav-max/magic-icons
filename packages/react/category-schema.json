				"3": "Three",
				"4": "Four",
				"5": "Five",
				"6": "Six",
				"7": "Seven",
				"8": "Eight",
				"9": "Nine"
			},
			"specialCharacters": {
				" ": "",
				"-": "",
				"_": ""
			},
			"reservedWords": ["default", "class", "export", "import", "return"]
		},
		"variants": {
			"suffixPattern": "{IconName}{Variant}",
